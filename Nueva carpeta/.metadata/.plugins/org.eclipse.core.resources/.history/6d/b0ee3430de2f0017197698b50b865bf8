package herencia;

public class Persona {

	String dni;
	String nombre;
	String apellido1;
	String apellido2;

	public Persona(String dni, String nombre, String apellido1, String apellido2) {
		this.dni = dni;
		this.nombre = nombre;
		this.apellido1 = apellido1;
		this.apellido2 = apellido2;
	}

}
